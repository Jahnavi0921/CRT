// find the roots of quadratic equation
#include <stdio.h>
#include<math.h>
int main(){
    float a,b,c; 
    scanf("%f%f%f",&a,&b,&c);
    float d=b*b-4*a*c;
    if(d<0){
        printf("roots not possible");
    }
    else{
        float r1=(-b+sqrt(d))/(2*a);
        float r2=(-b-sqrt(d))/(2*a);
        printf("%.1f\n",r1);
        printf("%.1f",r2);
    }
    

    
}
