// read mxn matrix and print its sum of diagonal elements
#include <stdio.h>

int main() {
    int m,n;
    scanf("%d%d",&m,&n);
    int i,j,a[m][n];
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
        scanf("%d",&a[i][j]);
        }
    }
    int s=0;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            if(i==j)
        s=s+a[i][j];
        }
    }
        printf("%d",s);
        return 0;
}
