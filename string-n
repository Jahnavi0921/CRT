// input:k4s3b5
//output:kkkksssbbbbb
#include <stdio.h>
#include<string.h>

int main() {
    char str1[20];
    scanf("%[^\n]s",str1);
    int i,n,j;
    n=strlen(str1);
    for(i=1;i<n;i=i+2){
        int r=(int)str1[i]-48;
        for(j=1;j<=r;j++){
            printf("%c",str1[i-1]);
        }
    }
    return 0;
}
